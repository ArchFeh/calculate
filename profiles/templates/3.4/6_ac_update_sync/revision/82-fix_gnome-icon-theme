# Calculate ini(overlay-calculate.revision)<82 exec=/bin/bash

#-ini(overlay-calculate.revision,82)-#

#?pkg(x11-themes/gnome-icon-theme)!=#
for delpath in /usr/share/icons/HighContrast/index.theme
do
test -f /var/db/pkg/x11-themes/gnome-icon-theme-*/CONTENTS &&
   sed -i "/ ${delpath////\\/} /d" /var/db/pkg/x11-themes/gnome-icon-theme-*/CONTENTS
done
#pkg#

exit 0
