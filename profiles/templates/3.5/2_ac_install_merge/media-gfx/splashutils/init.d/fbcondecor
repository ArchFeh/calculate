# Calculate multiline format=patch
<reg>(if ! fbcondecor_set_theme "\$\{theme\}" "\$\{TTY\}")(; then)</reg>
<text>\1 &amp;&gt;/dev/null \2</text>
<reg>sbin/runscript</reg>
<text>sbin/openrc-run</text>
<reg>^(\s+)(if fbcondecor_supported.*)(;\s+then)$</reg>
<text>\1\2 &amp;&amp;
\1	[ -e /sys/class/graphics/fb0/device/driver ] &amp;&amp;
\1	[ "$(basename `readlink /sys/class/graphics/fb0/device/driver`)" != "vboxvideo" ]\3</text>
